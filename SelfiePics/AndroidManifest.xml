<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.devpkjain.instaapp"
      android:versionCode="13"
      android:versionName="1.9"

    >

    <uses-feature android:name="android.hardware.camera"></uses-feature>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-sdk android:minSdkVersion="7"></uses-sdk>
    <application android:label="selfie-pics"
                 android:icon="@drawable/icon"
                 android:theme="@android:style/Theme.Light.NoTitleBar"
            >
        <activity android:name="com.devpkjain.instaapp.TakePictureActivity"
                android:configChanges="keyboardHidden|orientation">
        </activity>
        <activity android:name="com.devpkjain.instaapp.ImageListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.devpkjain.instaapp."/>
        <activity android:name="com.devpkjain.instaapp.ImageGridActivity"/>
        <activity android:name="com.devpkjain.instaapp.DashboardActivitys"/>
    </application>
</manifest> 
